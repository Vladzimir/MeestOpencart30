{{ header }}{{ column_left }}
<div id="content">
    <div class="page-header">
        <div class="container-fluid">
            <div class="row">
                <h1 class="meest-heading">{{ heading_title }} {{ heading_title_order_update_form }}</h1>
                <div class="pull-right">
                    <button type="submit" form="form-meest-cn" id="button-save-cn">{{ button_save_cn }}</button>
                </div>
            </div>
            <div class="row">
                <ul class="breadcrumb">
                    {% for breadcrumb in breadcrumbs %}
                        <li><a href="{{ breadcrumb.href }}">{{ breadcrumb.text }}</a></li>
                    {% endfor %}
                </ul>
            </div>
        </div>
    </div>
{#    <div class="container-fluid">#}
    {% if get_info_for_edit_error %}
        <div class="alert alert-danger alert-dismissible"><i class="fa fa-exclamation-circle"></i> {{ get_info_for_edit_error }}
            <button type="button" class="close" data-dismiss="alert">&times;</button>
        </div>
    {% endif %}
    {% if response.status == 'OK' %}
        {% for error in error_warning %}
            <div class="alert alert-danger">
                <i class="fa fa-exclamation-circle"></i> {{ error }}
                <button type="button" class="close" data-dismiss="alert">&times;</button>
            </div>
        {% endfor %}
{#        <div class="panel panel-default">#}
{#            <div class="panel-heading">#}
{#                <h3 class="panel-title"><i class="fa fa-file-text-o" aria-hidden="true"></i> {{ text_form }}</h3>#}
{#            </div>#}
            <div class="panel-body">
                <input type="hidden" name="error_meesage" id="error_meesage" value="{{ error_meesage }}">
                <form action="{{ action }}" method="post" enctype="multipart/form-data" id="form-meest-cn" class="form-horizontal">
                    <input type="hidden" name="sender_delivery_type" id="sender_delivery_type" value="{{ parcel_info.sender.service == 'Branch' ? 'department' : 'doors' }}">
                    <input type="hidden" name="recipient_delivery_type" id="recipient_delivery_type" value="{{ parcel_info.receiver.service == 'Branch' ? 'department' : 'doors' }}">
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="panel-heading clearfix">
                                <h3 class="panel-title">{{ text_sender }}</h3>
                                <div class="btn-group pull-right" data-toggle="buttons">
                                    <label class="btn btn-default btn-sm{{ parcel_info.sender.service == 'Branch' ? ' active' }}" data-toggle="tooltip" title="{{ button_department_delivery }}">
                                        <input type="radio" name="sender_address_type" value="department">
                                        <span class="btn-text">{{ text_branch }}</span>
                                    </label>
                                    <label class="btn btn-default btn-sm{{ parcel_info.sender.service == 'Door' ? ' active' }}" data-toggle="tooltip" title="{{ button_doors_delivery }}">
                                        <input type="radio" name="sender_address_type" value="doors">
                                        <span class="btn-text">{{ text_doors }}</span>
                                    </label>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-body" id="sender_department_delivery" style="{{ parcel_info.sender.service != 'Branch' ? ' display: none;' }}">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender_person">{{ entry_sender }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_person" value="{{ parcel_info.sender.name }}" placeholder="{{ entry_phone }}" id="input-sender_person" class="form-control custom-input-70"/>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender_phone">{{ entry_phone }}</label>
                                        <div class="col-sm-9">
                                             <input type="text" name="sender_phone" value="{{ parcel_info.sender.phone }}" placeholder="{{ entry_phone }}" id="input-sender_phone" class="form-control custom-input-70"/>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-region">{{ entry_sender_region }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="shipping_meest2_sender_region" id="input-sender-region" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                                {% for region in regions %}
                                                    <option value="{{ region.region_id }}" {% if region.region_id == branch_sender_info.region_id %}selected{% endif %}>
                                                        {{ region.region_name_ua }}
                                                    </option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-city">{{ entry_sender_city }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="shipping_meest2_sender_city" id="input-sender-city" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-branch">{{ entry_branch }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="sender_branch" id="input-sender-branch" class="form-control selectpicker show-tick custom-select2" data-live-search="true">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-body" id="sender_address_delivery" style="{{ parcel_info.sender.service != 'Door' ? ' display: none;' }}">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender_person-address">{{ entry_sender }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_person-address" value="{{ parcel_info.sender.name }}" placeholder="{{ entry_phone }}" id="input-sender_person" class="form-control custom-input-70"/>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender_phone-address">{{ entry_phone }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_phone-address" value="{{ parcel_info.sender.phone }}" placeholder="{{ entry_phone }}" id="input-sender_phone" class="form-control custom-input-70"/>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-region-address">{{ entry_sender_region }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="sender-region-address" id="input-sender-region-address" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                                {% for region in regions %}
                                                    <option value="{{ region.region_id }}" {% if region.region_id == sender_region_id_address %}selected{% endif %}>
                                                        {{ region.region_name_ua }}
                                                    </option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-city-address">{{ entry_sender_city }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="sender_city_address" id="input-sender-city-address" class="form-control select2 custom-select2">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-address">{{ text_address }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="sender_address" id="input-sender-address" class="form-control custom-select2">
                                                    <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-building">{{ text_building }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_building" id="input-sender-building" class="form-control custom-input-70" {% if parcel_info.sender.building %}value="{{ parcel_info.sender.building }}"{% endif %} placeholder="{{ text_building }}">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-floor">{{ text_floor }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_floor" id="input-sender-floor" class="form-control custom-input-70"  {% if parcel_info.sender.floor %}value="{{ parcel_info.sender.floor }}"{% endif %} placeholder="{{ text_floor }}">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-sender-apartment">Apartment:</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="sender_apartment" id="input-sender-apartment" class="form-control custom-input-70"  {% if parcel_info.sender.flat %}value="{{ parcel_info.sender.flat }}"{% endif %} placeholder="Apartment">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="panel-heading clearfix">
                                <h3 class="panel-title">{{ text_recipient }}</h3>
                                <div class="btn-group pull-right" data-toggle="buttons">
                                    <label class="btn btn-default btn-sm{{ parcel_info.receiver.service == 'Branch' ? ' active' }}" data-toggle="tooltip" title="{{ button_recipient_department_delivery }}">
                                        <input type="radio" name="recipient_address_type" value="department">
                                        <span class="btn-text">{{ text_branch }}</span>
                                    </label>
                                    <label class="btn btn-default btn-sm{{ parcel_info.receiver.service == 'Door' ? ' active' }}" data-toggle="tooltip" title="{{ button_recipient_doors_delivery }}">
                                        <input type="radio" name="recipient_address_type" value="doors">
                                        <span class="btn-text">{{ text_doors }}</span>
                                    </label>
                                </div>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-body" id="recipient_department_delivery" style="{{ parcel_info.receiver.service != 'Branch' ? ' display: none;' }}">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient">{{ entry_recipient }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient" value="{{ parcel_info.receiver.name }}" placeholder="{{ entry_recipient }}" id="input-recipient" class="form-control custom-input-70" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_phone">{{ entry_phone }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient_phone" value="{{ parcel_info.receiver.phone }}" placeholder="{{ entry_phone }}" id="input-recipient_phone" class="form-control custom-input-70" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_region">{{ entry_region }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_region" id="input-recipient_region" class="form-control custom-select2">
                                                {% for region in regions %}
                                                <option value="{{ region.region_id }}" {% if region.region_id == branch_recipient_info.region_id %}selected{% endif %}>
                                                    {{ region.region_name_ua }}
                                                </option>
                                                {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_city">{{ entry_city }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_city" id="input-recipient_city" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-branch" data-toggle="tooltip" data-placement="right" title="{{ recipient_department_name }}">{{ entry_branch }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_branch" id="input-recipient-branch" class="form-control selectpicker show-tick custom-select2" data-live-search="true">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="panel-body" id="recipient_address_delivery" style="{{ parcel_info.receiver.service != 'Door' ? ' display: none;' }}">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-name-address">{{ entry_recipient }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient-address" value="{{ parcel_info.receiver.name }}" placeholder="{{ entry_recipient }}" id="input-recipient" class="form-control custom-input-70" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_phone-address">{{ entry_phone }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient_phone-address" value="{{ parcel_info.receiver.phone }}" placeholder="{{ entry_phone }}" id="input-recipient_phone" class="form-control custom-input-70" />
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_region_address">{{ entry_region }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_region_address" id="input-recipient_region_address" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                                    {% for region in regions %}
                                                        <option value="{{ region.region_id }}" {% if region.region_id == receiver_region_id_address %}selected{% endif %}>
                                                            {{ region.region_name_ua }}
                                                        </option>
                                                    {% endfor %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient_city_address">{{ entry_city }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_city_address" id="input-recipient_city_address" class="form-control custom-select2">
                                                {% if receiver_city_id_address %}
                                                    <option value="">{{ text_select }}</option>
                                                    {% for city in cities %}
                                                        {% if city.city_id == receiver_city_id_address %}
                                                            <option value="{{ city.city_id }}" selected>
                                                                {{ city.name_ua }}
                                                            </option>
                                                        {% endif %}
                                                    {% endfor %}
                                                {% else %}
                                                    <option value="">{{ text_select }}</option>
                                                {% endif %}
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-address">{{ text_address }}</label>
                                        <div class="col-sm-9 text-right">
                                            <select name="recipient_address" id="input-recipient-address" class="form-control custom-select2">
                                                <option value="">{{ text_select }}</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-building-address">{{ text_building }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient_building_address" id="input-recipient-building-address" class="form-control custom-input-70" {% if parcel_info.receiver.building %}value="{{ parcel_info.receiver.building }}"{% endif %}placeholder="{{ text_building }}">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-floor-address">{{ text_floor }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient_floor_address" id="input-recipient-floor-address" class="form-control custom-input-70" {% if parcel_info.receiver.building %}value="{{ parcel_info.receiver.floor }}"{% endif %}placeholder="{{ text_floor }}">
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-recipient-apartment-address">{{ text_apartment }}</label>
                                        <div class="col-sm-9">
                                            <input type="text" name="recipient_apartment_address" id="input-recipient-apartment-address" class="form-control custom-input-70" {% if parcel_info.receiver.building %}value="{{ parcel_info.receiver.flat }}"{% endif %} placeholder="{{ text_apartment }}">
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-sm-6">
                            <div class="panel-heading clearfix">
                                <h3 class="panel-title">{{ text_departure_options }}</h3>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <div id="places-container">
                                        {% for place in parcel_info.placesItems %}
                                            <div class="place-item" data-index="{{ loop.index0 }}">
                                                <legend class="seat-legend">{{ text_seat }}</legend>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_weight }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][weight]" value="{{ place.weight }}" placeholder="{{ entry_weight }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_kg }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_length }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][length]" value="{{ place.length }}" placeholder="{{ entry_length }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_cm }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_width }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][width]" value="{{ place.width }}" placeholder="{{ entry_width }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_cm }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_height }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][height]" value="{{ place.height }}" placeholder="{{ entry_height }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_cm }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_quantity }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][quantity]" value="{{ place.quantity }}" placeholder="{{ entry_quantity }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_pc }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <label class="col-sm-3 control-label">{{ entry_insurance }}</label>
                                                    <div class="col-sm-9">
                                                        <div class="input-group">
                                                            <input type="text" name="places[{{ loop.index0 }}][insurance]" value="{{ place.insurance }}" placeholder="{{ entry_insurance }}" class="form-control custom-input-70"/>
                                                            <span class="input-group-addon addon-blue">{{ text_uah }}</span>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group">
                                                    <div class="col-sm-9 col-sm-offset-3 text-right">
{#                                                        <button type="button" class="btn btn-danger remove-place" style="display: {{ loop.length > 1 ? 'inline-block' : 'none' }};">{{ text_delete_seat }}</button>#}
                                                        <button type="button" class="btn btn-danger remove-place" style="display: none">{{ text_delete_seat }}</button>
                                                    </div>
                                                </div>
                                            </div>
                                        {% endfor %}
                                    </div>
                                    <div class="btn-container">
                                        <button type="button" class="btn btn-success pull-right" id="add-place">{{ text_add_a_seat }}</button>
                                    </div>
                                </div>

                            </div>
                        </div>
                        <div class="col-sm-6">
                            <div class="panel-heading clearfix">
                                <h3 class="panel-title">{{ text_payment }}</h3>
                            </div>
                            <div class="panel panel-default">
                                <div class="panel-body">
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-delivery_payer">{{ entry_delivery_payer }}</label>
                                        <div class="col-sm-9">
                                            <div class="btn-group" data-toggle="buttons">
                                                {% for v in references['payer_types'] %}
                                                    {% if v.Ref == 'Receiver' and parcel_info.receiverPay %}
                                                        <label class="btn btn-default active">
                                                            <input type="radio" name="delivery_payer" value="{{ v.Ref }}" id="input-delivery_payer-{{ v.Ref }}" checked>{{ v.Description }}
                                                        </label>
                                                    {% elseif v.Ref == 'Sender' and not parcel_info.receiverPay %}
                                                        <label class="btn btn-default active">
                                                            <input type="radio" name="delivery_payer" value="{{ v.Ref }}" id="input-delivery_payer-{{ v.Ref }}" checked>{{ v.Description }}
                                                        </label>
                                                    {% else %}
                                                        <label class="btn btn-default">
                                                            <input type="radio" name="delivery_payer" value="{{ v.Ref }}" id="input-delivery_payer-{{ v.Ref }}">{{ v.Description }}
                                                        </label>
                                                    {% endif %}
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="input-payment_type">{{ entry_payment_type }}</label>
                                        <div class="col-sm-9">
                                            <div class="btn-group" data-toggle="buttons">
                                                {% for v in references['payment_types'] %}
                                                    {% if v.Ref == parcel_info.payType %}
                                                        <label class="btn btn-default active">
                                                            <input type="radio" name="payment_type" value="{{ v.Ref }}" id="input-payment_type-{{ v.Ref }}" checked>{{ v.Description }}
                                                        </label>
                                                    {% else %}
                                                        <label class="btn btn-default">
                                                            <input type="radio" name="payment_type" value="{{ v.Ref }}" id="input-payment_type-{{ v.Ref }}">{{ v.Description }}
                                                        </label>
                                                    {% endif %}
                                                {% endfor %}
                                            </div>
                                        </div>
                                    </div>
                                    <div class="form-group">
                                        <label class="col-sm-3 control-label" for="toggle-cod">{{ text_cod }}</label>
                                        <div class="col-sm-9">
                                            <label class="switch">
                                                <input type="checkbox" id="toggle-cod" name="cod_toggle">
                                                <span class="slider round"></span>
                                            </label>
                                        </div>
                                    </div>
                                    <div class="cod-amount-container" style="display: none;">
                                        <div class="form-group">
                                            <label class="col-sm-3 control-label" for="input-cod-amount">{{ text_cod_amount }}</label>
                                            <div class="col-sm-9">
                                                <div class="input-group">
                                                    <input type="text" id="input-cod-amount" name="cod_amount" placeholder="{{ text_enter_cod_amount }}" class="custom-input-70" />
                                                    <span class="input-group-addon addon-blue">{{ text_uah }}</span>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="form-group card-number-container">
                                            <label class="col-sm-3 control-label" for="input-card-number">{{ text_card_number }}</label>
                                            <div class="col-sm-9">
                                                <input type="text" id="input-card-number" name="card_number" placeholder="{{ text_enter_card_number }}" class="custom-input-70" />
                                            </div>
                                        </div>
